{
  "manifest_version": 3,
  "short_name": "React App",
  "name": "Create React App Sample",
  "description": "A Chrome extension built with React.",
  "version": "3.0",
  "omnibox": { "keyword" : "aaron" },
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo512.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "favicon.ico",
      "48": "logo192.png",
      "128": "logo512.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab",
  "webNavigation","tabs"]
}
